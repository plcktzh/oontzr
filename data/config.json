            }, {
                "name": "numEvents",
                "initialValue": 4
            }, {
                "name": "patternOffset",
                "initialValue": 0
            }, {
                "name": "doRandomizeVelocities",
                "initialValue": false
            }, {
                "name": "doRandomize",
                "initialValue": false
            }]
        },
        "random": {
            "type": "random",
