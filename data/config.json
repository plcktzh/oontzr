                "initialValue": 16
            }, {
                "name": "patternOffset",
                "initialValue": 0
            }, {
                "name": "doRandomizeVelocities",
                "initialValue": false
            }, {
                "name": "doRandomize",
                "initialValue": false
            }]
        },
        "cellular": {
            "type": "cellular",
            "parameters": [{
                "name": "patternLength",
                "initialValue": 64
            }, {
                "name": "numSeeds",
                "initialValue": 1
            }, {
                "name": "doCenterSeeds",
                "initialValue": true
            }, {
                "name": "doRandomizeVelocities",
                "initialValue": false
            }, {
                "name": "wrapAround",
                "initialValue": false
            }, {
                "name": "doRandomize",
                "initialValue": true
            }]
        }
    },
    "pattern-parameters": {
        "velocity-min": 16,
        "velocity-max": 127,
        "length-min": 1,
        "length-max": 64
    },
    "prefixes": {
        "pattern": "ptn_",
        "step": "stp_",
        "sample": "smp_",
        "canvas": "cnv_"
    },
    "canvas-attributes": {
        "step-width": 24,
        "step-height": 32,
        "step-gap": 2,
        "canvas-padding": 8,
        "step-color": {
